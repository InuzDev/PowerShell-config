{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "foreground": "#DEFFF2",
          "background": "#446aa3",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "prefix": "<parentBackground>\ue0b0</>",
            "root_icon": "\uf292"
          },
          "foreground": "#fffb38",
          "background": "#ef5350",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "enable_hyperlink": true,
            "style": "full"
          },
          "foreground": "#2e2e2e",
          "powerline_symbol": "\ue0b0",
          "background": "#DEFFF2",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "template": "{{ HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}|{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Stagin.String }}</>{{ end }}"
          },
          "foreground": "#d7d8da",
          "powerline_symbol": "\ue0b0",
          "background": "#464F51",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahed 0 }}#c792ea{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "postfix": " ",
            "prefix": " \uf5ef "
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#1a1919",
          "background": "#60AFFF",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "postfix": "",
            "prefix": ""
          },
          "template": "\u2570\u2500",
          "foreground": "#ae8fd6",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "display_exit_code": true,
            "error_color": "#ef5350",
            "status_template": "‚ùØ"
          },
          "foreground": "#e0f8ff",
          "type": "status",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
